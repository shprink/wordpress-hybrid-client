{
  "name": "wordpress-hybrid-client",
  "version": "3.0.0-alpha01",
  "description": "Build amazing iOS and Android applications for your WordPress website for free!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shprink/wordpress-hybrid-client.git"
  },
  "keywords": [
    "wp-api",
    "angular",
    "angularjs",
    "rest",
    "restfull",
    "hybrid",
    "material design",
    "client"
  ],
  "author": "Julien Renaux <contact@julienrenaux.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shprink/wordpress-hybrid-client/issues"
  },
  "homepage": "https://github.com/shprink/wordpress-hybrid-client#readme",
  "scripts": {
    "start": "ionic serve",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "build:pwa": "npm run ionic:build --prod",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "postinstall": "npm run installWPHC",
    "installWPHC": "cd scripts && node ./install.js"
  },
  "dependencies": {
    "@angular/common": "^2.2.1",
    "@angular/compiler": "^2.2.1",
    "@angular/compiler-cli": "^2.2.1",
    "@angular/core": "^2.2.1",
    "@angular/forms": "^2.2.1",
    "@angular/http": "^2.2.1",
    "@angular/platform-browser": "^2.2.1",
    "@angular/platform-browser-dynamic": "^2.2.1",
    "@angular/platform-server": "^2.2.1",
    "@ionic/storage": "^1.1.9",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.2",
    "angular2-moment": "^1.1.0",
    "deep-extend": "^0.4.1",
    "ionic-angular": "^2.1.0",
    "ionic-native": "^2.5.1",
    "ionicons": "3.0.0",
    "lodash": "^4.17.3",
    "ng2-translate": "^5.0.0",
    "rxjs": "5.0.0-beta.12",
    "wp-api-angular": "^3.0.0-beta7",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^1.0.0",
    "cordova": "^6.5.0",
    "cson": "^4.0.0",
    "cson-loader": "^0.1.0",
    "file-loader": "^0.9.0",
    "strip-module-export-loader": "^0.1.0",
    "sw-toolbox": "^3.4.0",
    "typescript": "2.0.9"
  },
  "cordovaPlugins": [
    "cordova-plugin-whitelist",
    "cordova-plugin-device",
    "cordova-plugin-statusbar",
    "cordova-plugin-console",
    "cordova-plugin-splashscreen",
    "ionic-plugin-keyboard",
    "cordova-plugin-crosswalk-webview",
    "cordova-plugin-x-toast"
  ],
  "cordovaPlatforms": [
    {
      "platform": "android",
      "version": "6.x",
      "locator": "android@6.x"
    },
    {
      "platform": "ios",
      "version": "4.x",
      "locator": "ios@4.x"
    }
  ],
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "config": {
    "ionic_webpack": "./webpack/config.js"
  }
}
